<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  全白 透明色  -->
    <SolidColorBrush x:Key="TransparentColor" Color="#00FFFFFF" />
    <!--  全黑 透明色  -->
    <SolidColorBrush x:Key="TransparentBlackColor" Color="#00000000" />
    <!--  主题色  -->
    <SolidColorBrush x:Key="ThemeColor" Color="#2196F3" />
    <!--  按钮前景色  -->
    <SolidColorBrush x:Key="ButtonForeground" Color="#FFFFFF" />
    <!--  按钮背景色  -->
    <SolidColorBrush x:Key="ButtonBackground" Color="#FF4493" />
    <!--  按钮边框色  -->
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#FF4493" />
</ResourceDictionary>